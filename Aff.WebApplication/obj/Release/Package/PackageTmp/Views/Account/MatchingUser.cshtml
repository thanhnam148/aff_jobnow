@model Aff.Models.Models.UserMatchinhchModel
@{
    ViewBag.Title = "MatchingUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="m-portlet m-portlet--mobile">
    <div class="m-portlet__head">
        <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
                <h3 class="m-portlet__head-text">
                    Danh sách người dùng đăng ký
                </h3>
            </div>
        </div>
    </div>
    <div class="m-portlet__body">
        <!--begin: Search Form -->
        <form class="m-search__form m-form" method="post">
            @Html.AntiForgeryToken()
            <div class="row">
                <div id="updatemessage" class="m-alert m-alert--outline alert alert-dismissible m--hide" role="alert" style="width:90%;">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"></button>
                    <span class="message"></span>
                </div>
            </div>
            <div class="m-form m-form--label-align-right m--margin-top-0 m--margin-bottom-20">
                <div class="row align-items-center">
                    <div class="col-xl-9 order-2 order-xl-1">
                        <div class="form-group m-form__group row align-items-center">
                            <div class="col-md-3">
                                <div class="m-input-icon m-input-icon--left" style="display:none;">
                                    <input type="text" class="form-control m-input m-input--solid" placeholder="Tìm kiếm..." id="generalSearch2">
                                    <span class="m-input-icon__icon m-input-icon__icon--left">
                                        <span>
                                            <i class="la la-search"></i>
                                        </span>
                                    </span>
                                </div>
                            </div>
                            <div class="row align-items-center">
                                <div class="col-lg-5">
                                    <div class="input-group date" id="m_datepicker_123">
                                        <input type="text" data-placement="top" data-toggle="m-tooltip" title="" class="form-control m-input" id="DataOfMonth" name="DataOfMonth" readonly value="@Model.DataOfMonth" data-original-title="Lấy dữ liệu trong tháng">
                                        <span class="input-group-addon">
                                            <i class="la la-calendar"></i>
                                        </span>
                                    </div>
                                    <div class="d-md-none m--margin-bottom-10"></div>
                                </div>
                                @*<div class="col-lg-4">
                                <div class="input-group m_datepicker date" id="m_datepicker_3">
                                    <input type="text" data-placement="top" data-toggle="m-tooltip" title="" class="form-control m-input" id="txtToDate" data-original-title="Lấy dữ liệu đến ngày">
                                    <span class="input-group-addon">
                                        <i class="la la-calendar"></i>
                                    </span>
                                </div>
                                <div class="d-md-none m--margin-bottom-10"></div>
                            </div>*@
                                <div class="col-lg-4">
                                    <div class="col-md-4 col-sm-4"></div>
                                    <div class="form-group m-form__group m-form__group--inline ">
                                        <div class="m-demo__preview  m-demo__preview--btn text-white">
                                            <button class="btn btn-info m-btn  m-btn--air" id="btnGetData">
                                                <span>
                                                    <i class="fa fa-search"></i>
                                                    <span>
                                                        Lấy dữ liệu
                                                    </span>
                                                </span>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="text-right">
                                <div class="col-lg-4">
                                    <div class="form-group m-form__group m-form__group--inline ">
                                        <div class="m-demo__preview  m-demo__preview--btn text-white">
                                            <a href="#" class="btn m-btn m-btn--gradient-from-danger m-btn--gradient-to-warning btn-show-model" data-toggle="modal" data-target="#m_modal_3">
                                                <span>
                                                    <i class="fa fa-gear"></i>
                                                    <span>
                                                        Đối soát
                                                    </span>
                                                </span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                        </div>
                    </div>

                    <div class="col-xl-4 order-1 order-xl-2 m--align-right">

                        <div class="m-separator m-separator--dashed d-xl-none"></div>
                    </div>
                </div>
            </div>
        
            <!--begin: Datatable -->
            <div class="m_datatable nopaging" id="ajax_data"></div>
            <!--end: Datatable -->
        </form>
        
        <!--end: Search Form -->
        
    </div>
    <div class="modal fade" id="m_modal_3" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                        Bạn có chắc chắn muốn đối soát số dư không?
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">
                            &times;
                        </span>
                    </button>
                </div>
                
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">
                        Close
                    </button>
                    <button type="button" class="btn btn-primary" id="btnProcessData">
                        Tiếp tục
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="~/assets/scripts/datepicker-vi.js" type="text/javascript"></script>
    <script src="~/assets/demo/default/custom/components/forms/widgets/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="~/assets/scripts/matching-user-ajax.js" type="text/javascript"></script>
}




